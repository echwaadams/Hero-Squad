{{#partial "content"}}
   <section class="landing-pg">
       <div class="land-left>
       </div>
        <div class="land-right>
        </div>
        <h2> <a class="btns" href="/hero-form">Create Your Hero</a><br><br>CURRENT HEROES</h2>
        <div class="hero">
           {{#each hero}}
               <div class="hero-item">
                  <img src="/images/img1.jpeg" alt="">
                  <p>Name: {{name}}</p>
                  <p>Age: {{age}}</p>
                  <p> Power: {{power}}</p>
                  <p> Weakness: {{weakness}}</p>
                  <p> Id: {{id}}</p>
                  <a href="new/{{id}}" class="btns">Hero Stats</a>
                  <a href="/squad" class="btns">Add {{name}} To Squad</a>
               </div>
           {{/each}}
        </div>
   </section>
   {{/partial}}
   {{> layout.hbs}}